type User {
id: ID!
name: String!
email: String!
}

input CreateUserInput {
name: String!
email: String!
}

type Query {
users: [User!]!
userById(id: ID!): User
}

type Mutation {
createUser(input: CreateUserInput!): User!
updateUser(id: ID!, input: CreateUserInput!): User!
deleteUser(id: ID!): Boolean!
}